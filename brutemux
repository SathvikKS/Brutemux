cd $HOME/Brutemux
curl -LO https://raw.githubusercontent.com/SathvikKS/T-Load/master/T-Embed/port
curl -LO https://raw.githubusercontent.com/SathvikKS/Brutemux/master/Version/version1/version
https://raw.githibusercontent.com/SathvikKS/Brutemux/core
chmod +x version
chmod +x core
./core
clear
./version
echo "Select your choice"
echo "1) Facebook"
echo "2) Exit"
read -p ">|" choice
if [ $choice = "1" ]
then
cd facebook
clear
./facebook.sh
sleep 2
clear
bruteforce
elif [ $choice = "2" ]
then
exit
else
echo "Wrong Choice"
brutemux
fi
